  },

  {
    "_subst": true,
    "name": "SmtpEmail",
    "type": "email",
    "driverClass": "{{BUBBLE_SMTP_DRIVER}}",
    "driverConfig": {
      "tlsEnabled": true
    },
    "credentials": {
      "params": [
        {"name": "user", "value": "{{BUBBLE_SMTP_USER}}"},
        {"name": "password", "value": "{{BUBBLE_SMTP_PASS}}"},
        {"name": "host", "value": "{{BUBBLE_SMTP_SERVER}}"},
        {"name": "port", "value": "{{BUBBLE_SMTP_PORT}}"}
