[{
  "name": "UserBlocker",
  "children": {
    "AppMatcher": [{
      "name": "HNCommentMatcher",
      "site": "HackerNews",
      "template": true,
      "requestCheck": true,
      "requestModifier": true,
      "fqdn": "news.ycombinator.com",
      "urlRegex": "/item\\?id=\\d+",
      "rule": "hn_user_blocker"
    }, {
      "name": "HNThreadsMatcher",
      "site": "HackerNews",
      "template": true,
      "requestCheck": true,
      "requestModifier": true,
      "fqdn": "news.ycombinator.com",
      "urlRegex": "/threads\\?id=\\w+",
      "rule": "hn_user_blocker"
    }]
  }
}]
