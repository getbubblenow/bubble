[{
  "name": "UserBlocker",
  "children": {
    "AppMatcher": [{
      "name": "TwitterMatcher",
      "site": "Twitter",
      "template": true,
      "requestCheck": true,
      "requestModifier": true,
      "fqdn": "twitter.com",
      "urlRegex": ".*",
      "rule": "twitter_user_blocker"
    }, {
      "name": "TwitterMobileMatcher",
      "site": "Twitter",
      "template": true,
      "requestCheck": true,
      "requestModifier": true,
      "fqdn": "mobile.twitter.com",
      "urlRegex": ".*",
      "rule": "twitter_user_blocker"
    }]
  }
}]
