[{
  "name": "UserBlocker",
  "children": {
    "AppMatcher": [{
      "name": "ReasonMatcher",
      "site": "Reason",
      "template": true,
      "connCheck": true,
      "requestCheck": true,
      "requestModifier": true,
      "fqdn": "reason.com",
      "urlRegex": "/20\\d{2}/\\d{2}/\\d{2}/[-\\w]+/",
      "rule": "reason_user_blocker"
    }]
  }
