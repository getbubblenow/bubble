[{
  "name": "UserBlocker",
  "children": {
    "AppMatcher": [{
      "name": "FBMatcher",
      "site": "Facebook",
      "template": true,
      "requestCheck": true,
      "requestModifier": true,
      "fqdn": "www.facebook.com",
      "urlRegex": "/",
      "rule": "fb_user_blocker"
    }, {
      "name": "FBMobileMatcher",
      "site": "Facebook",
      "template": true,
      "requestCheck": true,
      "requestModifier": true,
      "fqdn": "m.facebook.com",
      "urlRegex": "/home.php",
      "rule": "fb_user_blocker"
    }]
  }
}]
